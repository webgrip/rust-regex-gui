{
  "branches": [
    "main"
  ],
  "tagFormat": "v${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "semantic-release-cargo",
      {
        "publish": true,
        "cargoPublish": true,
        "registryUrl": "https://crates.io"
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "Cargo.lock",
          "Cargo.toml",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "successCmd": "echo \"version=${nextRelease.version}\" >> $GITHUB_OUTPUT"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/mybin-x86_64-unknown-linux-gnu.tar.gz",
            "label": "Linux x86-64"
          },
          {
            "path": "dist/mybin-x86_64-apple-darwin.tar.gz",
            "label": "macOS x86-64"
          },
          {
            "path": "dist/mybin-x86_64-pc-windows-gnu.zip",
            "label": "Windows x86-64"
          },
          {
            "path": "dist/mybin-wasm32-unknown-unknown.wasm.gz",
            "label": "WebAssembly (wasm32)"
          }
        ]
      }
    ]
  ]
}
